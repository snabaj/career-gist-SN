    "@types/axios": "^0.9.36",
    "@types/sequelize": "^4.28.20",
    "concurrently": "^8.2.2",
    "nodemon": "^3.1.3"
  },
  "dependencies": {
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.13.1",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.5",
    "wait-on": "^8.0.2"
  }
